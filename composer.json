{
    "name": "microwin7/texture-provider",
    "description": "Предосталяет реализацию GravitLauncher TextureProvider и TextureLoader JSON",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "microwin7/php-utils": "^1.6.0.11",
        "symfony/console": "^7.0",
        "sentry/sentry": "^4.6",
        "firebase/php-jwt": "^6.10",
        "nesbot/carbon": "^3.0",
        "guzzlehttp/psr7": "^2.6"
    },
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Microwin7\\TextureProvider\\": [
                "src/",
                "config/"
            ],
            "Microwin7\\PHPUtils\\Configs\\": "config/php-utils/^1.6.0/",
            "Microwin7\\PHPUtils\\Exceptions\\Handler\\": "src/Exceptions/Handler/"
        }
    },
    "authors": [
        {
            "name": "microwin7",
            "email": "usa.microwin8@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "vimeo/psalm": "^5.22.1"
    },
    "bin": [
        ".bin/index",
        ".bin/index.bat"
    ]
}
