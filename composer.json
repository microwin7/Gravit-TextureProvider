{
    "name": "microwin7/texture-provider",
    "description": "Предосталяет реализацию GravitLauncher TextureProvider JSON",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "microwin7/php-utils": "dev-main",
        "symfony/console": "7.0.x-dev",
        "sentry/sentry": "dev-master",
        "firebase/php-jwt": "dev-main"
    },
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Microwin7\\TextureProvider\\": [
                "src/",
                "config/"
            ],
            "Microwin7\\PHPUtils\\Configs\\": "config/php-utils/^1.6.0/",
            "Microwin7\\PHPUtils\\Exceptions\\Handler\\": "src/Exceptions/Handler/"
        }
    },
    "authors": [
        {
            "name": "microwin7",
            "email": "usa.microwin8@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "vimeo/psalm": "5.x-dev"
    },
    "bin": [
        ".bin/index",
        ".bin/index.bat"
    ]
}
